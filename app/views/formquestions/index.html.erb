<p id="notice"><%= notice %></p>

<h1>Formquestions</h1>

<table>
  <thead>
    <tr>
      <th>Question name</th>
      <th>Form</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @formquestions.each do |formquestion| %>
      <tr>
        <td><%= formquestion.question_name %></td>
        <td><%= formquestion.form_id %></td>
        <td><%= link_to 'Show', formquestion %></td>
        <td><%= link_to 'Destroy', formquestion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Formquestion', new_formquestion_path %>
